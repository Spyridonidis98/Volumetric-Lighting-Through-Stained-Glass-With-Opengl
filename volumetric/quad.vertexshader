#version 330 core
layout(location = 0) in vec3 vertexPosition_modelspace;
layout(location = 1) in vec3 vertexNormal_modelspace;
layout(location = 2) in vec2 vertexUV;

out vec2 vertex_UV;

void main()
{
    vertex_UV = vertexUV;
    gl_Position = vec4(vertexPosition_modelspace.x, vertexPosition_modelspace.y, 0.0, 1.0); 
}